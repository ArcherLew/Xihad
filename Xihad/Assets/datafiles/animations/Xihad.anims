		<Subscription event="MapEffectUpToOne" action="Stop" />
		<Subscription event="MapEffectUpToTwo" action="Start" />
		<Subscription event="MapEffectUpToThree" action="Stop" />
		<Subscription event="MapEffectDownToOne" action="Stop" />
		<Subscription event="MapEffectDownToTwo" action="Stop" />
		<Subscription event="MapEffectDownToThree" action="Stop" />
		<Subscription event="MapEffectToZero" action="Stop" />
	</AnimationDefinition>
	
