	<AnimationDefinition name="MapEffectDownToOne" autoStart="false" duration="1.0" replayMode="loop">
		<Affector property="RightIconVertFormat" interpolator="String" applicationMethod="absolute">
			<KeyFrame position="0" value="TopAligned" />
		</Affector>
		<Affector property="RightIcon" interpolator="String" applicationMethod="absolute">
			<KeyFrame position="0" value="XihadUI/Map/effect_down_one" />
		</Affector>
		<Subscription event="MapEffectUpToOne" action="Stop" />
		<Subscription event="MapEffectUpToTwo" action="Stop" />
		<Subscription event="MapEffectUpToThree" action="Stop" />
		<Subscription event="MapEffectDownToOne" action="Start" />
		<Subscription event="MapEffectDownToTwo" action="Stop" />
		<Subscription event="MapEffectDownToThree" action="Stop" />
		<Subscription event="MapEffectToZero" action="Stop" />
	</AnimationDefinition>
	
	<AnimationDefinition name="MapEffectDownToTwo" autoStart="false" duration="1.0" replayMode="loop">
		<Affector property="RightIconVertFormat" interpolator="String" applicationMethod="absolute">
			<KeyFrame position="0" value="TopAligned" />
		</Affector>
		<Affector property="RightIcon" interpolator="String" applicationMethod="absolute">
			<KeyFrame position="0" value="XihadUI/Map/effect_down_one" />
			<KeyFrame position="0.2" value="XihadUI/Map/effect_down_two" />
		</Affector>
		<Subscription event="MapEffectUpToOne" action="Stop" />
		<Subscription event="MapEffectUpToTwo" action="Stop" />
		<Subscription event="MapEffectUpToThree" action="Stop" />
		<Subscription event="MapEffectDownToOne" action="Stop" />
		<Subscription event="MapEffectDownToTwo" action="Start" />
		<Subscription event="MapEffectDownToThree" action="Stop" />
		<Subscription event="MapEffectToZero" action="Stop" />
	</AnimationDefinition>
	
	<AnimationDefinition name="MapEffectDownToThree" autoStart="false" duration="1.0" replayMode="loop">
		<Affector property="RightIconVertFormat" interpolator="String" applicationMethod="absolute">
			<KeyFrame position="0" value="TopAligned" />
		</Affector>
		<Affector property="RightIcon" interpolator="String" applicationMethod="absolute">
			<KeyFrame position="0" value="XihadUI/Map/effect_down_one" />
			<KeyFrame position="0.2" value="XihadUI/Map/effect_down_two" />
			<KeyFrame position="0.4" value="XihadUI/Map/effect_down_three" />
		</Affector>
		<Subscription event="MapEffectUpToOne" action="Stop" />
		<Subscription event="MapEffectUpToTwo" action="Stop" />
		<Subscription event="MapEffectUpToThree" action="Stop" />
		<Subscription event="MapEffectDownToOne" action="Stop" />
		<Subscription event="MapEffectDownToTwo" action="Stop" />
