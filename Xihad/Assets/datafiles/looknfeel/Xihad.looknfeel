<?xml version="1.0" ?>
<Falagard version="7">
	
	<!--
	*****************************************************************
		Xihad/Container
	*****************************************************************
	-->
	 <WidgetLook name="Xihad/Container">
		<PropertyDefinition name="VertImageFormat" redrawOnWrite="true" initialValue="Tiled" type="VerticalFormatting" />
		<PropertyDefinition name="HorzImageFormat" redrawOnWrite="true" initialValue="Tiled" type="HorizontalFormatting" />
		<PropertyDefinition name="Image" redrawOnWrite="true" initialValue="" type="Image" />
		<PropertyDefinition name="TextColor" redrawOnWrite="true" initialValue="FF412502" type="Colour" />
		<PropertyDefinition name="VertTextFormat" redrawOnWrite="true" initialValue="BottomAligned" type="VerticalTextFormatting" />
		<PropertyDefinition name="HorzTextFormat" redrawOnWrite="true" initialValue="LeftAligned" type="HorizontalTextFormatting" />
		<Property name="Font" value="fangzheng-14" />
		<ImagerySection name="background">
			<ImageryComponent>
				<Area>
					<Dim type="LeftEdge">
						<AbsoluteDim value="0"/>
					</Dim>
					<Dim type="TopEdge">
						<AbsoluteDim value="0"/>
					</Dim>
					<Dim type="RightEdge">
						<UnifiedDim scale="1" type="RightEdge"/>
					</Dim>
					<Dim type="BottomEdge">
						<UnifiedDim scale="1" type="BottomEdge"/>
					</Dim>
				</Area>
				<ImageProperty name="Image"/>
				<VertFormatProperty name="VertImageFormat"/>
				<HorzFormatProperty name="HorzImageFormat"/>
			</ImageryComponent>
		</ImagerySection>
		<ImagerySection name="text">
			<TextComponent>
				<Area>
					<Dim type="LeftEdge">
						<AbsoluteDim value="0"/>
					</Dim>
					<Dim type="TopEdge">
						<AbsoluteDim value="0"/>
					</Dim>
					<Dim type="RightEdge">
						<UnifiedDim scale="1" type="RightEdge"/>
					</Dim>
					<Dim type="BottomEdge">
						<UnifiedDim scale="1" type="BottomEdge"/>
					</Dim>
				</Area>

				<VertFormatProperty name="VertTextFormat"/>
				<HorzFormatProperty name="HorzTextFormat"/>
			</TextComponent>
		</ImagerySection>
		
		<StateImagery name="Enabled">
			<Layer>
				<Section section="background"/>
				<Section section="text">
					<ColourProperty name="TextColor" />
				</Section>
			</Layer>
		</StateImagery>
		<StateImagery name="Disabled">
			<Layer>
				<Section section="background"/>
				<Section section="text">
					<ColourProperty name="TextColor" />
				</Section>
			</Layer>
		</StateImagery>
	</WidgetLook>

		
	
	<!--
	*****************************************************************
		Xihad/DialogNameCard
	*****************************************************************
	-->
	
	<WidgetLook name="Xihad/DialogNameCard">
		<PropertyDefinition name="VertLabelFormatting" initialValue="CentreAligned" redrawOnWrite="true" type="VerticalTextFormatting"/>
		<PropertyDefinition name="HorzLabelFormatting" initialValue="CentreAligned" redrawOnWrite="true" type="HorizontalTextFormatting"/>
		<!-- <Property name="Font" value=""> -->
		<ImagerySection name="leftAlignedLabel">
			<TextComponent>
				<Area>
					<Dim type="LeftEdge"><ImageDim name="XihadUI/Dialog/dialog_nameL" dimension="Width"/></Dim>
					<Dim type="TopEdge"><AbsoluteDim value="0"/></Dim>
					<Dim type="Width" ><FontDim type="HorzExtent"/></Dim>
					<Dim type="Height"><FontDim type="LineSpacing" padding="8" /></Dim>
				</Area>
				<VertFormatProperty name="VertLabelFormatting" />
				<HorzFormatProperty name="HorzLabelFormatting" />
			</TextComponent>
		</ImagerySection>
		<ImagerySection name="leftAlignedFrame">
			<FrameComponent>
				<Area>
					<Dim type="LeftEdge"><AbsoluteDim value="0"/></Dim>
					<Dim type="TopEdge"><AbsoluteDim value="0"/></Dim>
					<Dim type="Width">
						<OperatorDim op="Add">
							<FontDim type="HorzExtent"/>
							<OperatorDim op="Add">
								<ImageDim name="XihadUI/Dialog/dialog_nameL" dimension="Width"/>
								<ImageDim name="XihadUI/Dialog/dialog_nameR" dimension="Width"/>
							</OperatorDim>	
						</OperatorDim>
					</Dim>
					<Dim type="Height"><FontDim type="LineSpacing" padding="8" /></Dim>
				</Area>
				<Image component="TopLeftCorner" name="XihadUI/Dialog/dialog_nameLT"/>
				<Image component="TopRightCorner" name="XihadUI/Dialog/dialog_nameRT"/>
				<Image component="BottomLeftCorner" name="XihadUI/Dialog/dialog_nameLB"/>
				<Image component="BottomRightCorner" name="XihadUI/Dialog/dialog_nameRB"/>
				<Image component="LeftEdge" name="XihadUI/Dialog/dialog_nameL"/>
				<Image component="RightEdge" name="XihadUI/Dialog/dialog_nameR"/>
				<Image component="TopEdge" name="XihadUI/Dialog/dialog_nameT"/>
				<Image component="BottomEdge" name="XihadUI/Dialog/dialog_nameB"/>
				<Image component="Background" name="XihadUI/Dialog/dialog_nameM"/>
			</FrameComponent>
		</ImagerySection>
		<ImagerySection name="rightAlignedLabel">
			<TextComponent>
				<Area>
					<Dim type="LeftEdge">
						<OperatorDim op="Subtract">
							<UnifiedDim scale="1" type="RightEdge"/>
								<OperatorDim op="Add">
									<FontDim type="HorzExtent"/>
									<ImageDim name="XihadUI/Dialog/dialog_nameR" dimension="Width"/>
								</OperatorDim>  
						</OperatorDim>
					</Dim>
					<Dim type="TopEdge"><AbsoluteDim value="0"/></Dim>
					<Dim type="Width" ><FontDim type="HorzExtent"/></Dim>
					<Dim type="Height"><FontDim type="LineSpacing" padding="8" /></Dim>
				</Area>
				<VertFormatProperty name="VertLabelFormatting" />
				<HorzFormatProperty name="HorzLabelFormatting" />
			</TextComponent>
		</ImagerySection>
		<ImagerySection name="rightAlignedFrame">
			<FrameComponent>
				<Area>
					<Dim type="TopEdge"><AbsoluteDim value="0"/></Dim>
					<Dim type="LeftEdge">
						<OperatorDim op="Subtract">
							<UnifiedDim scale="1" type="RightEdge"/>
							<OperatorDim op="Add">
								<FontDim type="HorzExtent"/>
								<OperatorDim op="Add">
									<ImageDim name="XihadUI/Dialog/dialog_nameL" dimension="Width"/>
									<ImageDim name="XihadUI/Dialog/dialog_nameR" dimension="Width"/>
								</OperatorDim>  
							</OperatorDim>
						</OperatorDim>
					</Dim>
					<Dim type="Width">
						<OperatorDim op="Add">
							<FontDim type="HorzExtent"/>
							<OperatorDim op="Add">
								<ImageDim name="XihadUI/Dialog/dialog_nameL" dimension="Width"/>
								<ImageDim name="XihadUI/Dialog/dialog_nameR" dimension="Width"/>
							</OperatorDim>  
						</OperatorDim>
					</Dim>
					<Dim type="Height"><FontDim type="LineSpacing" padding="8" /></Dim>
				</Area>
				<Image component="TopLeftCorner" name="XihadUI/Dialog/dialog_nameLT"/>
				<Image component="TopRightCorner" name="XihadUI/Dialog/dialog_nameRT"/>
				<Image component="BottomLeftCorner" name="XihadUI/Dialog/dialog_nameLB"/>
				<Image component="BottomRightCorner" name="XihadUI/Dialog/dialog_nameRB"/>
				<Image component="LeftEdge" name="XihadUI/Dialog/dialog_nameL"/>
				<Image component="RightEdge" name="XihadUI/Dialog/dialog_nameR"/>
				<Image component="TopEdge" name="XihadUI/Dialog/dialog_nameT"/>
				<Image component="BottomEdge" name="XihadUI/Dialog/dialog_nameB"/>
				<Image component="Background" name="XihadUI/Dialog/dialog_nameM"/>
			</FrameComponent>
		</ImagerySection>
		<StateImagery name="Enabled">
			<Layer>
				<Section section="leftAlignedFrame" />
				<Section section="leftAlignedLabel" />
			</Layer>
		</StateImagery>
		<StateImagery name="Disabled">
			<Layer>
				<Section section="rightAlignedFrame" />
				<Section section="rightAlignedLabel" />
			</Layer>
		</StateImagery>
	</WidgetLook>

	<!--
	*****************************************************************
		Xihad/Dialog
	*****************************************************************
	-->
		
	<WidgetLook name="Xihad/Dialog">
		<PropertyLinkDefinition name="CharacterName" widget="__auto_name_card__" targetProperty="Text" initialValue="？？？" type="String"/>
		<PropertyLinkDefinition name="NextButtonImage" widget="__auto_next__" targetProperty="Image" initialValue="XihadUI/Dialog/dialog_next_1" type="Image"/>
		<PropertyLinkDefinition name="NameCardRightAligned" widget="__auto_name_card__" targetProperty="Disabled" initialValue="false" type="bool"/>

		<Property name="AlwaysOnTop" value="true"/>
		<ImagerySection name="background">
			<FrameComponent>
				<Area>
					<Dim type="TopEdge" ><AbsoluteDim value="10"/></Dim>
				</Area>
				<Image component="TopLeftCorner" name="XihadUI/Dialog/dialogLT"/>
				<Image component="TopRightCorner" name="XihadUI/Dialog/dialogRT"/>
				<Image component="BottomLeftCorner" name="XihadUI/Dialog/dialogLB"/>
				<Image component="BottomRightCorner" name="XihadUI/Dialog/dialogRB"/>
				<Image component="LeftEdge" name="XihadUI/Dialog/dialogL"/>
				<Image component="RightEdge" name="XihadUI/Dialog/dialogR"/>
				<Image component="TopEdge" name="XihadUI/Dialog/dialogT"/>
				<Image component="BottomEdge" name="XihadUI/Dialog/dialogB"/>
				<Image component="Background" name="XihadUI/Dialog/dialogM"/>
			</FrameComponent>
		</ImagerySection>
		<Child type="Xihad/DialogNameCard" nameSuffix="__auto_name_card__">
			<Area>
				<Dim type="LeftEdge"><AbsoluteDim value="40"/></Dim>
				<Dim type="RightEdge">
					<OperatorDim op="Subtract">
						<UnifiedDim scale="1" type="RightEdge"/>
						<AbsoluteDim value="40"/>
					</OperatorDim>
				</Dim>
			</Area>
		</Child> 
		<Child type="Xihad/Container" nameSuffix="__auto_container__">
			<Area>
				<Dim type="LeftEdge"><ImageDim name="XihadUI/Dialog/dialogL" dimension="Width"/></Dim>
				<Dim type="RightEdge">
					<OperatorDim op="Subtract">
						<UnifiedDim scale="1" type="Width"/>
						<ImageDim name="XihadUI/Dialog/dialogR" dimension="Width"/>
					</OperatorDim>
				</Dim>
				<Dim type="TopEdge">
					<OperatorDim op="Add">
						<ImageDim name="XihadUI/Dialog/dialogT" dimension="Height"/>
						<AbsoluteDim value="10"/>
					</OperatorDim>
				</Dim>
				<Dim type="BottomEdge">
					<OperatorDim op="Subtract">
						<UnifiedDim scale="1" type="Height"/>
						<ImageDim name="XihadUI/Dialog/dialogB" dimension="Height"/>
					</OperatorDim>
				</Dim>
			</Area>
		</Child>
		<Child type="Xihad/Image" nameSuffix="__auto_next__">
			<Area>
				<Dim type="LeftEdge">
					<OperatorDim op="Subtract">
						<UnifiedDim scale="1" type="RightEdge"/>
						<OperatorDim op="Add">
							<ImageDim name="XihadUI/Dialog/dialogRB" dimension="Width"/>
							<ImageDim name="XihadUI/Dialog/dialog_next_1" dimension="Width"/>
						</OperatorDim>
					</OperatorDim>
				</Dim>
				<Dim type="TopEdge">
					<OperatorDim op="Subtract">
						<UnifiedDim scale="1" type="BottomEdge"/>
						<OperatorDim op="Add">
							<ImageDim name="XihadUI/Dialog/dialogRB" dimension="Height"/>
							<ImageDim name="XihadUI/Dialog/dialog_next_1" dimension="Height"/>
						</OperatorDim>
					</OperatorDim>
				</Dim>
				<Dim type="Width">
					<ImageDim name="XihadUI/Dialog/dialog_next_1" dimension="Width"/>
				</Dim>
				<Dim type="Height">
					<ImageDim name="XihadUI/Dialog/dialog_next_1" dimension="Height"/>
				</Dim>
			</Area>
			<Property name="AlwaysOnTop" value="true"/>
		</Child>
		<StateImagery name="Enabled">
			<Layer>
				<Section section="background">
				</Section>	
			</Layer>
		</StateImagery>
		<StateImagery name="Disabled">
			<Layer>
				<Section section="background">
				</Section>
			</Layer>
		</StateImagery>	

		<AnimationDefinition name="RotateNext" autoStart="true" duration="1.0" replayMode="loop">
			<Affector property="NextButtonImage" interpolator="String" applicationMethod="absolute">
				<KeyFrame position="0.0" value="XihadUI/Dialog/dialog_next_1" />
				<KeyFrame position="0.2" value="XihadUI/Dialog/dialog_next_1" />
				<KeyFrame position="0.3" value="XihadUI/Dialog/dialog_next_2" />
				<KeyFrame position="0.4" value="XihadUI/Dialog/dialog_next_3" />
				<KeyFrame position="0.5" value="XihadUI/Dialog/dialog_next_4" />
				<KeyFrame position="0.6" value="XihadUI/Dialog/dialog_next_3" />
				<KeyFrame position="0.7" value="XihadUI/Dialog/dialog_next_2" />
				<KeyFrame position="0.8" value="XihadUI/Dialog/dialog_next_1" />
				<KeyFrame position="1.0" value="XihadUI/Dialog/dialog_next_1" />
			</Affector>
		</AnimationDefinition>
	</WidgetLook>
	
