	<!--
	*****************************************************************
		Xihad/PopupMenu
	*****************************************************************
	-->
	<WidgetLook name="Xihad/PopupMenu">
		<PropertyDefinition name="BorderWidth" initialValue="0" layoutOnWrite="true" type="uint"/>
		<Property name="ItemSpacing" value="0" />
		<NamedArea name="ItemRenderArea">
			<Area>
				<Dim type="LeftEdge" >
					<OperatorDim op="Add">
						<AbsoluteDim value="0"/>
						<PropertyDim name="BorderWidth" />
					</OperatorDim>
				</Dim>
				<Dim type="TopEdge" >
					<OperatorDim op="Add">
						<AbsoluteDim value="0"/>
						<PropertyDim name="BorderWidth" />
					</OperatorDim>
				</Dim>
				<Dim type="RightEdge" >
					<OperatorDim op="Subtract">
						<UnifiedDim scale="1" type="RightEdge" />
						<OperatorDim op="Add">
							<ImageDim name="XihadUI/Menu/frameR" dimension="Width" />
							<PropertyDim name="BorderWidth" />
						</OperatorDim>
					</OperatorDim>
				</Dim>
				<Dim type="BottomEdge" >
					<OperatorDim op="Subtract">
						<UnifiedDim scale="1" type="BottomEdge" />
						<OperatorDim op="Add">
							<ImageDim name="XihadUI/Menu/frameB" dimension="Height" />
							<PropertyDim name="BorderWidth" />
						</OperatorDim>
					</OperatorDim>
				</Dim>
			</Area>
		</NamedArea>
		<ImagerySection name="frame">
			<FrameComponent>
				<Image component="BottomLeftCorner" name="XihadUI/Menu/frameLB" />
				<Image component="BottomRightCorner" name="XihadUI/Menu/frameRB" />
				<Image component="TopRightCorner" name="XihadUI/Menu/frameRT" />
				<Image component="RightEdge" name="XihadUI/Menu/frameR" />
				<Image component="BottomEdge" name="XihadUI/Menu/frameB" />
			</FrameComponent>
		</ImagerySection>
		<StateImagery name="Enabled">
			<Layer>
				<Section section="frame"/>
			</Layer>
		</StateImagery>
		<StateImagery name="Disabled">
			<Layer>
				<Section section="frame"/>
			</Layer>
		</StateImagery>
	</WidgetLook>

	<!--
	*****************************************************************
		Xihad/MenuItem
	*****************************************************************
	-->
	<WidgetLook name="Xihad/MenuItem">
		<PropertyDefinition name="NormalTextColour" initialValue="FF412502" redrawOnWrite="true" type="ColourRect"/>
		<PropertyDefinition name="HoverTextColour" initialValue="FFF6E3BA" redrawOnWrite="true" type="ColourRect"/>
		<PropertyDefinition name="PushedTextColour" initialValue="FFF6E3BA" redrawOnWrite="true" type="ColourRect"/>
		<PropertyDefinition name="DisabledTextColour" initialValue="FFA39079" redrawOnWrite="true" type="ColourRect"/>
		<PropertyDefinition name="PushedColour" initialValue="FFFFFFFF" redrawOnWrite="true" type="ColourRect"/>
		<PropertyDefinition name="OpenedColour" initialValue="FFFFFFFF" redrawOnWrite="true" type="ColourRect"/>
		<PropertyDefinition name="PopupCloseOffset" initialValue="0" redrawOnWrite="true" type="uint"/>
		<Property name="Font" value="fangzheng-14"/>
		<Property name="WantsMultiClickEvents" value="False" />
		<NamedArea name="ContentSize">
			<Area>
				<Dim type="Width">
					<OperatorDim op="Add">
						<FontDim type="HorzExtent" />
						<AbsoluteDim value="46"/>
					</OperatorDim>
				</Dim>
				<Dim type="Height">
					<OperatorDim op="Add">
						<FontDim type="LineSpacing" />
						<AbsoluteDim value="6" />
					</OperatorDim>
				</Dim>
			</Area>
		</NamedArea>
		<NamedArea name="HasPopupContentSize">
			<Area>
				<Dim type="Width">
					<OperatorDim op="Add">
						<AbsoluteDim value="46" />
						<OperatorDim op="Add">
							<FontDim type="HorzExtent" />
							<OperatorDim op="Add">
								<ImageDim name="XihadUI/Menu/popup_close" dimension="Width" />
								<AbsoluteDim value="3" />
							</OperatorDim>
						</OperatorDim>
					</OperatorDim>
				</Dim>
				<Dim type="Height">
					<OperatorDim op="Add">
						<AbsoluteDim value="6" />
						<FontDim type="LineSpacing" />
					</OperatorDim>
				</Dim>
			</Area>
		</NamedArea>
		<ImagerySection name="label">
			<TextComponent>
				<Area>
					<Dim type="LeftEdge"><AbsoluteDim value="6" /></Dim>
					<Dim type="TopEdge"><AbsoluteDim value="0" /></Dim>
					<Dim type="RightEdge"><UnifiedDim scale="1" type="RightEdge" /></Dim>
					<Dim type="BottomEdge"><UnifiedDim scale="1" type="BottomEdge" /></Dim>
				</Area>
				<VertFormat type="CentreAligned" />
				<HorzFormat type="LeftAligned" />
			</TextComponent>
		</ImagerySection>
		<ImagerySection name="normal_frame">
			<FrameComponent>
				<Image component="TopLeftCorner" name="XihadUI/Menu/itemLT"/>
				<Image component="TopRightCorner" name="XihadUI/Menu/itemRT"/>
				<Image component="BottomLeftCorner" name="XihadUI/Menu/itemLB"/>
				<Image component="BottomRightCorner" name="XihadUI/Menu/itemRB"/>
				<Image component="LeftEdge" name="XihadUI/Menu/itemL"/>
				<Image component="RightEdge" name="XihadUI/Menu/itemR"/>
				<Image component="TopEdge" name="XihadUI/Menu/itemT"/>
				<Image component="BottomEdge" name="XihadUI/Menu/itemB"/>
				<Image component="Background" name="XihadUI/Menu/itemM"/>
			</FrameComponent>
		</ImagerySection>
		<ImagerySection name="hover_frame">
			<FrameComponent>
				<Image component="TopLeftCorner" name="XihadUI/Menu/hover_itemLT"/>
				<Image component="TopRightCorner" name="XihadUI/Menu/hover_itemRT"/>
				<Image component="BottomLeftCorner" name="XihadUI/Menu/hover_itemLB"/>
				<Image component="BottomRightCorner" name="XihadUI/Menu/hover_itemRB"/>
				<Image component="LeftEdge" name="XihadUI/Menu/hover_itemL"/>
				<Image component="RightEdge" name="XihadUI/Menu/hover_itemR"/>
				<Image component="TopEdge" name="XihadUI/Menu/hover_itemT"/>
				<Image component="BottomEdge" name="XihadUI/Menu/hover_itemB"/>
				<Image component="Background" name="XihadUI/Menu/hover_itemM"/>
			</FrameComponent>
		</ImagerySection>
		<ImagerySection name="popup_close">
			<ImageryComponent>
				<Area>
					<Dim type="RightEdge">
						<OperatorDim op="Subtract">
							<UnifiedDim scale="1" type="Width" />
							<OperatorDim op="Add">
								<PropertyDim name="PopupCloseOffset"/>
								<AbsoluteDim value="3"/>
							</OperatorDim>
						</OperatorDim>
					</Dim>
				</Area>
				<Image name="XihadUI/Menu/popup_close" />
				<VertFormat type="CentreAligned" />
				<HorzFormat type="RightAligned" />
			</ImageryComponent>
		</ImagerySection>
		<StateImagery name="EnabledNormal">
			<Layer>
